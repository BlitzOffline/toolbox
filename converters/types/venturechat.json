{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/VentureChatConfig",
  "definitions": {
    "VentureChatConfig": {
      "type": "object",
      "properties": {
        "tellformatto": {
          "type": "string"
        },
        "tellformatfrom": {
          "type": "string"
        },
        "jsonformatting": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/VentureChatFormat"
          }
        }
      },
      "required": [
        "tellformatto",
        "tellformatfrom"
      ]
    },
    "VentureChatFormat": {
      "type": "object",
      "properties": {
        "priority": {
          "type": "number"
        },
        "json_attributes": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/VentureChatJsonComponent"
          }
        }
      },
      "required": [
        "priority"
      ]
    },
    "VentureChatJsonComponent": {
      "type": "object",
      "properties": {
        "hover_text": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "click_action": {
          "type": "string"
        },
        "click_text": {
          "type": "string"
        }
      },
      "required": [
        "hover_text",
        "click_action",
        "click_text"
      ]
    }
  }
}